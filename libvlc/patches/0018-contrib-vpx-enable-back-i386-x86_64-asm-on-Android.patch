Subject: [PATCH 18/19] contrib: vpx: enable back i386/x86_64 asm on Android

Since relocation issues are now fixed.
---
 contrib/src/vpx/rules.mak | 5 -----
 1 file changed, 5 deletions(-)

diff --git a/contrib/src/vpx/rules.mak b/contrib/src/vpx/rules.mak
index 846b1e4b92..9c12c052f4 100644
--- a/contrib/src/vpx/rules.mak
+++ b/contrib/src/vpx/rules.mak
@@ -136,11 +136,6 @@ endif
 endif
 endif
 
-ifneq ($(filter i386 x86_64,$(ARCH)),)
-# broken text relocations or invalid register for .seh_savexmm with gcc8
-VPX_CONF += --disable-mmx
-endif
-
 ifdef WITH_OPTIMIZATION
 VPX_CFLAGS += -DNDEBUG
 else
