Subject: [PATCH 16/19] contrib: ffmpeg: enable back x86_64 asm on Android

Since relocation issues are now fixed.
---
 contrib/src/ffmpeg/rules.mak | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/contrib/src/ffmpeg/rules.mak b/contrib/src/ffmpeg/rules.mak
index 4c0ef53ade..0aa82fdeac 100644
--- a/contrib/src/ffmpeg/rules.mak
+++ b/contrib/src/ffmpeg/rules.mak
@@ -153,9 +153,6 @@ ifdef HAVE_ANDROID
 ifeq ($(ANDROID_ABI), x86)
 FFMPEGCONF +=  --disable-mmx --disable-mmxext --disable-inline-asm
 endif
-ifeq ($(ANDROID_ABI), x86_64)
-FFMPEGCONF +=  --disable-mmx --disable-mmxext --disable-inline-asm
-endif
 endif
 
 # Windows
